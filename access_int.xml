<?xml version='1.0' encoding='utf-8'?>
<nc:config xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns:oc-eth="http://openconfig.net/yang/interfaces/ethernet" xmlns:oc-if="http://openconfig.net/yang/interfaces" xmlns:oc-vlan="http://openconfig.net/yang/vlan" xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type" xmlns:if="urn:ietf:params:xml:ns:yang:ietf-interfaces" xmlns:oc-ext="http://openconfig.net/yang/openconfig-ext" xmlns:oc-inet="http://openconfig.net/yang/types/inet" xmlns:oc-ip="http://openconfig.net/yang/interfaces/ip" xmlns:oc-lag="http://openconfig.net/yang/interfaces/aggregate" xmlns:oc-types="http://openconfig.net/yang/openconfig-types" xmlns:oc-vlan-types="http://openconfig.net/yang/vlan-types" xmlns:oc-yang="http://openconfig.net/yang/types/yang" xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types">
